%
% 左に0を詰めて指定桁数にする．
%   \Ofil#1#2
%     #1 : 指定桁数
%     #2 : 数字列
\def\Ofil#1#2{%
  \edef\Ofil@a{#2}%
  \Strlen\Ofil@a\Ofil@n
  \Cfor{\ISub{#1}\Ofil@n\Ofil@n}{\Ofil@n>0}{\Decr\Ofil@n}\do{0}%
  \Ofil@a}
\def\OfilStr#1#2#3{%
  \edef\Ofil@a{#2}%
  \Strlen\Ofil@a\Ofil@n
  \edef#3{}%
  \Cfor{\ISub{#1}\Ofil@n\Ofil@n}{\Ofil@n>0}{\Decr\Ofil@n}\do{%
    \edefappend#3{0}}%
  \edef#3{#3\Ofil@a}}
\def\Ofilii#1{\Ofil{2}{#1}}
\def\Ofiliii#1{\Ofil{3}{#1}}
\def\Ofiliv#1{\Ofil{4}{#1}}
